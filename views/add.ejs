<body>

    <!-- jQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <!-- Bootstrap JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <div class="container">
        <h1> Bread (browser,read,edit,add,delete)</h1>
        <h2> Filters </h2>
        <FORM method="post" action="/add" cellpadding="100">
            <table style="width:100%;" border="0">
                <tr>
                    <div class="form-group">
                        <td>String</td>
                        <td><input type="text" name="string" class="form-control" placeholder="String"></td>
                    </div>
                </tr>
                <tr>
                    <div class="form-group">
                        <td>Integer</td>
                        <td><input type="text" name="integer" class="form-control" placeholder="Integer"></td>
                    </div>
                </tr>
                <tr>
                    <div class="form-group">
                        <td>Float</td>
                        <td><input type="text" name="float" class="form-control" placeholder="Float"></td>
                    </div>
                </tr>
                <tr>
                    <div class="form-group">
                        <td>Date</td>
                        <td><input type="date" name="date" class="form-control" placeholder="Date"></td>
                    </div>
                </tr>
                <tr>
                    <div class="form-group">
                        <td>Boolean</td>
                        <td>
                            <select name="boolean" class="form-control">
                                <option value="" disabled selected>Choose the boolean ....</option>
                                <option value="True">True</option>
                                <option value="False">False</option>
                            </select>
                        </td>
                    </div>
                </tr>
                <tr>
                    <td><button type="submit" value="Save" class="btn btn-primary">Save</button></td>
                    <td></td>
                </tr>
            </table>
        </FORM>
    </div>

